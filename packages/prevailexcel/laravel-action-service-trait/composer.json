{
    "name": "aizaztag/package",
    "description": "simple Laravel package to create actions, traits and services using artisan commands",
    "autoload": {
        "psr-4": {
            "PrevailExcel\\ActionServiceTrait\\": "src/"
        },
        "classmap": ["src/"]
    },
    "authors": [
        {
            "name": "Aizaz"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PrevailExcel\\ActionServiceTrait\\ASTServiceProvider"
            ]
        }
    }
}
